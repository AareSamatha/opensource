N=int(input())
arr=list(map(int,input().split()))
proportion_of_positive=0
proportion_of_negative=0
proportion_of_zero=0
for i in arr:
    if i>0:
        proportion_of_positive+=1
    elif i<0:
        proportion_of_negative+=1
    else:
        proportion_of_zero+=1
print(proportion_of_positive/N)
print(proportion_of_negative/N)
print(proportion_of_zero/N)
